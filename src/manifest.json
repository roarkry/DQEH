{
  "manifest_version": 2,

  "name": "DEHQ Session Keep Alive",
  "description": "Keeps your DEHQ Session Alive",
  "version": "0.5",
  "background": {
	"scripts": ["jquery.min.js", "background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
	"tabs",
	"http://*/",
	"https://*/",
	"background"
  ]
}
